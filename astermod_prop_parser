# Create list of properties according to DAMIT
prop_list_parse = collect(m.match for m in eachmatch(r"(\"(.*?)\",)|(\"(.*?)\")|(,)", rd[1]))
prop_list = [strip(thing, ['\"', '\\', ',']) for thing in prop_list_parse]
