# Check if asteroid data file exists in current directory
if isfile("asteroids")
    # Open the file
    f = open("asteroids");
    rd = readlines(f);
    close(f)
else
    # Extract updated asteroid data from DAMIT
    ext_data = download("http://astro.troja.mff.cuni.cz/projects/asteroids3D/php/db_export_extended.php", "asteroids");
    # Open the file
    f = open("asteroids");
    rd = readlines(f);
    close(f)
end

k = ["aster_id",
